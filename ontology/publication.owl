<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY ss "http://semanticscience.org/resource/" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://semanticscience.org/ontology/"
     xml:base="http://semanticscience.googlecode.com/svn/trunk/ontology/publication.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ss="http://semanticscience.org/resource/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <rdfs:label
            >publication ontology</rdfs:label>
        <dc:identifier rdf:datatype="&xsd;anyURI"
            >http://semanticscience.org/document/publication.owl</dc:identifier>
        <owl:versionInfo>1.1</owl:versionInfo>
        <dc:date>2009-03-19</dc:date>
        <dc:creator
            >Michel Dumontier &lt;michel_dumontier@carleton.ca&gt;</dc:creator>
        <dc:title
            >An ontology for publications.</dc:title>
        <owl:imports rdf:resource="http://semanticscience.googlecode.com/svn/trunk/ontology/nulo.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;description"/>
    <owl:AnnotationProperty rdf:about="&ss;definition"/>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&dc;date"/>
    <owl:AnnotationProperty rdf:about="&dc;identifier"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://semanticscience.org/resource/Abstract -->

    <owl:Class rdf:about="&ss;Abstract">
        <rdfs:label xml:lang="en"
            >abstract section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition xml:lang="en"
            >An abstract section is a document section that provides brief summary of a document that explains the main argument(s), topic(s) or findings.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/AcknowledgementsSection -->

    <owl:Class rdf:about="&ss;AcknowledgementsSection">
        <rdfs:label xml:lang="en"
            >acknowledgements section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition xml:lang="en"
            >An acknowledgements section is a document section that identifies individuals, groups or organizations for their support in the development of the work.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Address -->

    <owl:Class rdf:about="&ss;Address">
        <rdfs:label>address</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;TextualEntity"/>
        <ss:definition
            >An address is a textual entity that denotes the physical location of some independent continuant.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Affiliation -->

    <owl:Class rdf:about="&ss;Affiliation">
        <rdfs:label xml:lang="en">affiliation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;TextualEntity"/>
        <rdfs:comment xml:lang="en"
            >An affiliation is a textual entity that describes the relation of the author or contributor with their place of work or study.</rdfs:comment>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Article -->

    <owl:Class rdf:about="&ss;Article">
        <rdfs:label xml:lang="en">article</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Publication"/>
        <ss:definition xml:lang="en"
            >An article is a publication that is stand-alone section of a larger work.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/AuthorContributionSection -->

    <owl:Class rdf:about="&ss;AuthorContributionSection">
        <rdfs:label
            >author contribution section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition
            >An author contribution section is a document section that describes the contribution of the authors.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/AuthorRole -->

    <owl:Class rdf:about="&ss;AuthorRole">
        <rdfs:label>author role</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Role"/>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/AuthorSection -->

    <owl:Class rdf:about="&ss;AuthorSection">
        <rdfs:label>author section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition
            >An author section is a document section that lists the contributing authors.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/BibliographySection -->

    <owl:Class rdf:about="&ss;BibliographySection">
        <rdfs:label xml:lang="en"
            >bibliography section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition xml:lang="en"
            >A bibliography section is a document section that is composed of a list of references used in the development of the work.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Blog -->

    <owl:Class rdf:about="&ss;Blog">
        <rdfs:label xml:lang="en">blog</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Publication"/>
        <ss:definition xml:lang="en"
            >A blog is a publication accessible at some website and is typically about various experiences.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Book -->

    <owl:Class rdf:about="&ss;Book">
        <rdfs:label xml:lang="en">book</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Publication"/>
        <ss:definition xml:lang="en"
            >A book is a publication composed of a large number of entries.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/BookSeries -->

    <owl:Class rdf:about="&ss;BookSeries">
        <rdfs:label xml:lang="en">series</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Publication"/>
        <ss:definition xml:lang="en"
            >A book series is a collection of books that have been sequentially published.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/BookVolume -->

    <owl:Class rdf:about="&ss;BookVolume">
        <rdfs:label>book volume</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Book"/>
        <ss:definition
            >A book volume is a single book that is part of a collection.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Booklet -->

    <owl:Class rdf:about="&ss;Booklet">
        <rdfs:label xml:lang="en">booklet</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Document"/>
        <ss:definition xml:lang="en"
            >A booklet is a document that is printed and bound, but lacks a named publisher or sponsoring institution.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Chapter -->

    <owl:Class rdf:about="&ss;Chapter">
        <rdfs:label xml:lang="en">chapter</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition xml:lang="en"
            >A chapter is a document section of a book or thesis.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Citation -->

    <owl:Class rdf:about="&ss;Citation">
        <rdfs:label>citation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;TextualEntity"/>
        <ss:definition
            >A citation is a textual entity denoting a source described in the bibliography or reference section of a document.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/College -->

    <owl:Class rdf:about="&ss;College">
        <rdfs:label xml:lang="en">college</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Organization"/>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/ConferenceProceeding -->

    <owl:Class rdf:about="&ss;ConferenceProceeding">
        <rdfs:label xml:lang="en">proceedings</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Book"/>
        <ss:definition xml:lang="en"
            >A proceedings is a book composed of papers presented at a conference.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/CopyrightSection -->

    <owl:Class rdf:about="&ss;CopyrightSection">
        <rdfs:label xml:lang="en"
            >copyright section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition xml:lang="en"
            >A copyright section is a document section that contains a notice of copyright.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/CorrespondanceSection -->

    <owl:Class rdf:about="&ss;CorrespondanceSection">
        <rdfs:label
            >correspondance section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition
            >A correspondence section is a document section that contains the details for contacting the corresponding author.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/DOI -->

    <owl:Class rdf:about="&ss;DOI">
        <rdfs:label>DOI</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentIdentifier"/>
        <ss:definition
            >A Digital Object Identifier (DOI) is an identifier for content-related entities on digital networks.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/DataItem -->

    <owl:Class rdf:about="&ss;DataItem">
        <rdfs:label>data item</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;InformationContentEntity"/>
        <ss:definition
            >A data item is an information content entity that either stands alone or is part of a data set or Record.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/DataSet -->

    <owl:Class rdf:about="&ss;DataSet">
        <rdfs:label>data set</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DataItem"/>
        <ss:definition
            >A data set is a collection of data items.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Diary -->

    <owl:Class rdf:about="&ss;Diary">
        <rdfs:label>diary</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Document"/>
        <ss:definition
            >A diary is a document which describes day-to-day experiences.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/DiscussionSection -->

    <owl:Class rdf:about="&ss;DiscussionSection">
        <rdfs:label xml:lang="en"
            >discussion section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition xml:lang="en"
            >The discussion section is a document section containing a summary of the findings, a reflection on the significance of findings, comparison with related work, among others.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Document -->

    <owl:Class rdf:about="&ss;Document">
        <rdfs:label xml:lang="en">document</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;InformationContentEntity"/>
        <ss:definition xml:lang="en"
            >A document is an information content entity that is compomsed of other information content entities intended to be understood together as a whole.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/DocumentIdentifier -->

    <owl:Class rdf:about="&ss;DocumentIdentifier">
        <rdfs:label
            >document identifier</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;RecordIdentifier"/>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/DocumentSection -->

    <owl:Class rdf:about="&ss;DocumentSection">
        <rdfs:label xml:lang="en"
            >document section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;InformationContentEntity"/>
        <rdfs:comment xml:lang="en"
            >A self-contained part of a larger written work.</rdfs:comment>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/DocumentTitle -->

    <owl:Class rdf:about="&ss;DocumentTitle">
        <rdfs:label xml:lang="en">document title</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;TextualEntity"/>
        <ss:definition xml:lang="en"
            >A document title is a  textual entity that summarizes the topic of the document in one sentence.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/EditedPublication -->

    <owl:Class rdf:about="&ss;EditedPublication">
        <rdfs:label xml:lang="en"
            >edited publication</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Publication"/>
        <ss:definition xml:lang="en"
            >An edited publication is a publication that has been examined and potentially changed by an editor.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Edition -->

    <owl:Class rdf:about="&ss;Edition">
        <rdfs:label>edition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Version"/>
        <ss:definition
            >An edition is a version of a literary work edited and published, as by a certain editor or in a certain manner including being printed during some interval of time.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/EditorRole -->

    <owl:Class rdf:about="&ss;EditorRole">
        <rdfs:label>editor role</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Role"/>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/EmailAddress -->

    <owl:Class rdf:about="&ss;EmailAddress">
        <rdfs:label>email address</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Identifier"/>
        <ss:definition
            >An email address is an identifier that provides information to an electronic mail delivery system.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/FirstName -->

    <owl:Class rdf:about="&ss;FirstName">
        <rdfs:label>first name</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Name"/>
        <ss:definition
            >A first name is a name that denotes a specific individual between members of a group of individuals, whose members usually share the same surname.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Group -->

    <owl:Class rdf:about="&ss;Group">
        <rdfs:label>group</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;MaterialEntity"/>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/HonorsThesis -->

    <owl:Class rdf:about="&ss;HonorsThesis">
        <rdfs:label xml:lang="en">honor&#39;s thesis</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;ThesisDocument"/>
        <ss:definition xml:lang="en"
            >An honor&#39;s thesis is a thesis prepared as a requirement for an honor&#39;s undergraduate degree.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/ICCN -->

    <owl:Class rdf:about="&ss;ICCN">
        <rdfs:label>ICCN</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;RecordIdentifier"/>
        <ss:definition
            >An Library of Congress Control Number (LCCN) is an identifier for numbering cataloging records in the United States Library of Congress.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/ISBN -->

    <owl:Class rdf:about="&ss;ISBN">
        <rdfs:label>ISBN</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentIdentifier"/>
        <ss:definition
            >An ISBN (International Standard Book Number) is a unique identifier for a commerical book.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/ISSN -->

    <owl:Class rdf:about="&ss;ISSN">
        <rdfs:label>ISSN</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentIdentifier"/>
        <ss:definition
            >An International Standard Serial Number (ISSN) is a unique identifier for print or electronic periodical publication.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Identifier -->

    <owl:Class rdf:about="&ss;Identifier">
        <rdfs:label>identifier</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Name"/>
        <ss:definition
            >An identifier is a name that uniquely identifies an entity.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/InformationContentEntity -->

    <owl:Class rdf:about="&ss;InformationContentEntity">
        <rdfs:label
            >information content entity</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Institute -->

    <owl:Class rdf:about="&ss;Institute">
        <rdfs:label xml:lang="en">institute</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Organization"/>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/IntroductionSection -->

    <owl:Class rdf:about="&ss;IntroductionSection">
        <rdfs:label xml:lang="en"
            >introduction section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition xml:lang="en"
            >An introduction section is a document section that generally provides background, motivation and goals of the work.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Journal -->

    <owl:Class rdf:about="&ss;Journal">
        <rdfs:label xml:lang="en">journal</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Publication"/>
        <ss:definition xml:lang="en"
            >A journal is a a peer-reviewed periodical in which scholarship relating to a particular academic discipline is published.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Label -->

    <owl:Class rdf:about="&ss;Label">
        <rdfs:label>label</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;TextualEntity"/>
        <ss:definition
            >A label is a textual entity that briefly describes or non-uniquely 
identifies some entity.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/LanguageLabel -->

    <owl:Class rdf:about="&ss;LanguageLabel">
        <rdfs:label>language label</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Label"/>
        <ss:definition
            >A language label is a word that denotes the language of a textual entity.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/LastName -->

    <owl:Class rdf:about="&ss;LastName">
        <rdfs:label>last name</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Name"/>
        <ss:definition
            >A surname is a name added to a given name and is part of a personal name and is often the family name.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/LegalName -->

    <owl:Class rdf:about="&ss;LegalName">
        <rdfs:label>legal name</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;PersonalName"/>
        <ss:definition
            >A legal name is a name given at birth, or which appears on their birth certificate, marriage certificate, or change of name certificate.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/List -->

    <owl:Class rdf:about="&ss;List">
        <rdfs:label>list</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DataSet"/>
        <ss:definition
            >A list is an ordered data set.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Magazine -->

    <owl:Class rdf:about="&ss;Magazine">
        <rdfs:label xml:lang="en">magazine</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Publication"/>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Manual -->

    <owl:Class rdf:about="&ss;Manual">
        <rdfs:label xml:lang="en">manual</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Publication"/>
        <ss:definition xml:lang="en"
            >A manual is a booklet that instructs on the usage of a device.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Manucript -->

    <owl:Class rdf:about="&ss;Manucript">
        <rdfs:label>manuscript</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Document"/>
        <ss:definition
            >A manuscript is a document prior to publication.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/MastersThesis -->

    <owl:Class rdf:about="&ss;MastersThesis">
        <rdfs:label xml:lang="en">master&#39;s thesis</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;ThesisDocument"/>
        <ss:definition xml:lang="en"
            >A Master&#39;s thesis is a thesis prepared as a requirement for a Master&#39;s degree.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/MaterialEntity -->

    <owl:Class rdf:about="&ss;MaterialEntity">
        <rdfs:label>material entity</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/MaterialsAndMethodsSection -->

    <owl:Class rdf:about="&ss;MaterialsAndMethodsSection">
        <rdfs:label xml:lang="en"
            >materials and methods section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition xml:lang="en"
            >The materials and methods section is a document section containing a description of the materials and methods used in the study.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/MaterialsSection -->

    <owl:Class rdf:about="&ss;MaterialsSection">
        <rdfs:label xml:lang="en"
            >materials section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition xml:lang="en"
            >The materials section is a document section containing a description of the materials used in the study.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/MethodsSection -->

    <owl:Class rdf:about="&ss;MethodsSection">
        <rdfs:label xml:lang="en">methods section</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;DocumentSection"/>
        <ss:definition xml:lang="en"
            >The methods section is a document section containing a description of the methods used in the study.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Name -->

    <owl:Class rdf:about="&ss;Name">
        <rdfs:label>name</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Label"/>
        <ss:definition
            >A name is a label used to distinguish one thing from another.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Newspaper -->

    <owl:Class rdf:about="&ss;Newspaper">
        <rdfs:label xml:lang="en">newspaper</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Periodical"/>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Note -->

    <owl:Class rdf:about="&ss;Note">
        <rdfs:label xml:lang="en">note</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;Document"/>
        <ss:definition xml:lang="en"
            >A note is a brief document.</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/Organization -->

    <owl:Class rdf:about="&ss;Organization">
        <rdfs:label xml:lang="en">organization</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;MaterialEntity"/>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/PMID -->

    <owl:Class rdf:about="&ss;PMID">
        <rdfs:label>PMID</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ss;RecordIdentifier"/>
        <ss:definition
            >A PubMed Identifier (PMID) is a unique identifer for publications in the sciences and biomedical scientific journal articles</ss:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/resource/PageRange -->

    <owl:Class rdf:about="&ss;PageRange">
        <rdfs:label>page range</rdfs:label>
