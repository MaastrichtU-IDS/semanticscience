<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY dl "http://ontology.dumontierlab.com/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY ss "http://semanticscience.org/resource/" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY bro "http://semanticscience.org/resource/bro.owl#" >
]>


<rdf:RDF xmlns="http://semanticscience.org/ontology/bro.owl"
     xml:base="http://semanticscience.googlecode.com/svn/trunk/ontology/bro.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:bro="&ss;bro.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ss="http://semanticscience.org/resource/"
     xmlns:dl="http://ontology.dumontierlab.com/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <dc:date rdf:datatype="&xsd;date">2008-01-01</dc:date>
        <dc:title rdf:datatype="&xsd;string"
            >Basic Relation Ontology</dc:title>
        <dc:creator rdf:datatype="&xsd;string"
            >Michel Dumontier</dc:creator>
        <dc:description rdf:datatype="&xsd;string"
            >The BRO provides a set of domain-independent relations.</dc:description>
        <dc:format rdf:datatype="&xsd;string"
            >application/rdf+xml</dc:format>
        <dc:language rdf:datatype="&xsd;string">en</dc:language>
        <dc:publisher rdf:datatype="&xsd;string"
            >semanticscience.org</dc:publisher>
        <owl:imports rdf:resource="http://semanticscience.googlecode.com/svn/trunk/ontology/annotation.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;language"/>
    <owl:AnnotationProperty rdf:about="&dc;description"/>
    <owl:AnnotationProperty rdf:about="&ss;definition"/>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&dc;publisher"/>
    <owl:AnnotationProperty rdf:about="&ss;example"/>
    <owl:AnnotationProperty rdf:about="&dc;date"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    <owl:AnnotationProperty rdf:about="&dc;format"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://semanticscience.org/resource/contains -->

    <owl:ObjectProperty rdf:about="&ss;contains">
        <rdfs:label rdf:datatype="&xsd;string">contains</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A spatial relation between two continuants such that one spatially contains the other with no overlap, and one is not a part of the other.</ss:definition>
        <owl:inverseOf rdf:resource="&ss;isContainedIn"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isSpatiallyRelatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/denotes -->

    <owl:ObjectProperty rdf:about="&ss;denotes">
        <rdfs:label>denotes</rdfs:label>
        <ss:example
            >The word &#39;Boston&#39; denotes the city of Boston by virtue of it being concretized as words on various signs around the city.</ss:example>
        <ss:definition
            >g denotes c when a generically dependent continuant g is concretized by a specifically dependent continuant s which inheres in independent continuant c.</ss:definition>
        <rdfs:subPropertyOf rdf:resource="&ss;isAbout"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/derivativeOf -->

    <owl:ObjectProperty rdf:about="&ss;derivativeOf">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">derivative of</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A transitive, symmetric, temporal relation in which one continuant is the derivative of another non-identical continuant.</ss:definition>
        <owl:inverseOf rdf:resource="&ss;derivativeOf"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isTemporallyRelatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/derivesFrom -->

    <owl:ObjectProperty rdf:about="&ss;derivesFrom">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">derives from</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A transitive, temporal relation in which one continuant derives from another non-identical continuant.</rdfs:comment>
        <ss:example rdf:datatype="&xsd;string"
            >glucose-6-phosphate derivesFrom glucose in glucose metabolism</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;derivativeOf"/>
        <owl:inverseOf rdf:resource="&ss;derivesInto"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&ss;immediatelyDerivesFrom"/>
            <rdf:Description rdf:about="&ss;immediatelyDerivesFrom"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/derivesInto -->

    <owl:ObjectProperty rdf:about="&ss;derivesInto">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">derives into</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A transitive, temporal relation in which one continuant derives into another non-identical continuant.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&ss;derivativeOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasAgent -->

    <owl:ObjectProperty rdf:about="&ss;hasAgent">
        <rdf:type rdf:resource="&owl;AsymmetricProperty"/>
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:label>has agent</rdfs:label>
        <ss:definition
            >A relation between a process and an active continuant.</ss:definition>
        <rdfs:subPropertyOf rdf:resource="&ss;hasParticipant"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasDisposition -->

    <owl:ObjectProperty rdf:about="&ss;hasDisposition">
        <rdfs:label rdf:datatype="&xsd;string">has disposition</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A relation between an entity and the disposition it realizes.</ss:definition>
        <rdfs:subPropertyOf rdf:resource="&ss;has_inherent"/>
        <owl:inverseOf rdf:resource="&ss;isDispositionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasFunction -->

    <owl:ObjectProperty rdf:about="&ss;hasFunction">
        <rdfs:label rdf:datatype="&xsd;string">has function</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A relation between an entity and a function.</ss:definition>
        <ss:example
            >vase hasFunction containerFunction</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;hasDisposition"/>
        <owl:inverseOf rdf:resource="&ss;isFunctionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasImproperPart -->

    <owl:ObjectProperty rdf:about="&ss;hasImproperPart">
        <rdfs:label rdf:datatype="&xsd;string"
            >has improper part</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A reflexive mereological relational such that the entities are not distinct.</ss:definition>
        <ss:example rdf:datatype="&xsd;string"
            >cake has improper part slice of cake</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;hasPart"/>
        <owl:inverseOf rdf:resource="&ss;isImproperPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasNonTangentialProperPart -->

    <owl:ObjectProperty rdf:about="&ss;hasNonTangentialProperPart">
        <rdfs:label
            >has non-tangential proper part</rdfs:label>
        <rdfs:comment
            >RCC8:non-tangential proper part</rdfs:comment>
        <ss:definition
            >A proper part relation in which there is no overlap of the edge of spatial regions.</ss:definition>
        <rdfs:subPropertyOf rdf:resource="&ss;hasProperPart"/>
        <owl:inverseOf rdf:resource="&ss;isNonTangentialProperPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasPart -->

    <owl:ObjectProperty rdf:about="&ss;hasPart">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">has part</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >has part is a reflexive (for all p, p has part p), transitive (for all p, p1, p2  if p part_of p1 and p1 part_of p2, then p part_of p2) and anti-symmetric (for all p, p1, if p part_of p1 and p1 part_of p then p and p1 are identical) relation.</ss:definition>
        <rdfs:comment
            >Due to nonstructural restrictions in OWL2, no cardinality restriction may be added to this property due to involvement in property chains. One should use hasProperPart to quantify over. hasPart is effectively reflexive due to the reflexivity of hasImproperPart</rdfs:comment>
        <ss:example
            >person has part arm, workshop has part coffee break</ss:example>
        <owl:inverseOf rdf:resource="&ss;isPartOf"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isRelatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasParticipant -->

    <owl:ObjectProperty rdf:about="&ss;hasParticipant">
        <rdfs:label rdf:datatype="&xsd;string">has participant</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A relation between a process and its a continuant participant.</ss:definition>
        <rdfs:comment
            >1. no cardinality restriction may be added to this property due to involvement in role chains. One should use hasAgent/realizes to specify knowledge and to quantify over.</rdfs:comment>
        <owl:inverseOf rdf:resource="&ss;isParticipantIn"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isRelatedTo"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&ss;hasPart"/>
            <rdf:Description rdf:about="&ss;hasParticipant"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasProperPart -->

    <owl:ObjectProperty rdf:about="&ss;hasProperPart">
        <rdf:type rdf:resource="&owl;AsymmetricProperty"/>
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">has proper part</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A mereological relation such that when x has proper part y, then x and y are distinct, and the spatial region occupied by y is wholly contained by with that of x.</ss:definition>
        <ss:example
            >car hasProperPart engine</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;hasPart"/>
        <owl:inverseOf rdf:resource="&ss;isProperPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasQuality -->

    <owl:ObjectProperty rdf:about="&ss;hasQuality">
        <rdfs:label rdf:datatype="&xsd;string">has quality</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A relation between an entity and the quality that inheres in it.</ss:definition>
        <ss:example
            >bob hasQuality HappinessQuality</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;has_inherent"/>
        <owl:inverseOf rdf:resource="&ss;isQualityOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasReference -->

    <owl:ObjectProperty rdf:about="&ss;hasReference">
        <rdfs:label>has reference</rdfs:label>
        <ss:definition
            >A relation between an entity and a source of information.</ss:definition>
        <owl:inverseOf rdf:resource="&ss;isReferenceFor"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isRelatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasRole -->

    <owl:ObjectProperty rdf:about="&ss;hasRole">
        <rdfs:label rdf:datatype="&xsd;string">has role</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >The relation between an entity and a role.</ss:definition>
        <ss:example
            >bob hasRole teacherRole</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;has_inherent"/>
        <owl:inverseOf rdf:resource="&ss;isRoleOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/hasTangentialProperPart -->

    <owl:ObjectProperty rdf:about="&ss;hasTangentialProperPart">
        <rdfs:label
            >has tangential proper part</rdfs:label>
        <ss:definition
            >A proper part relation in which there is overlap on the edge of a spatial region.</ss:definition>
        <rdfs:comment
            >RCC8:tangential proper part</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&ss;hasProperPart"/>
        <owl:inverseOf rdf:resource="&ss;isTangentialProperPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/has_inherent -->

    <owl:ObjectProperty rdf:about="&ss;has_inherent">
        <rdfs:label>has inherent</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&ss;isBearerOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/immediatelyDerivesFrom -->

    <owl:ObjectProperty rdf:about="&ss;immediatelyDerivesFrom">
        <rdfs:label rdf:datatype="&xsd;string"
            >immediately derives from</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A non-transitive temporal relation in which one continuant immediately derives from a non-identical continuant such that there are no (known) intermediate continuants between them.</rdfs:comment>
        <ss:example rdf:datatype="&xsd;string"
            >a molecule of alpha-D-glucose-6-phosphate immediately_derives_from D-glucose in glucose metabolism.</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;derivesFrom"/>
        <owl:inverseOf rdf:resource="&ss;immediatelyDerivesInto"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/immediatelyDerivesInto -->

    <owl:ObjectProperty rdf:about="&ss;immediatelyDerivesInto">
        <rdfs:label rdf:datatype="&xsd;string"
            >immediately derives into</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A non-transitive temporal relation in which one continuant immediately derives into a non-identical continuant such that there are no intermediate steps between them.</rdfs:comment>
        <ss:example rdf:datatype="&xsd;string"
            >glucose immediatelyDerivesInto glucose-6-phosphate in glucose metabolism</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;derivesInto"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/immediatelyPrecededBy -->

    <owl:ObjectProperty rdf:about="&ss;immediatelyPrecededBy">
        <rdfs:label rdf:datatype="&xsd;string"
            >immediately preceded by</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A non-transitive temporal relation in which one process is immediately preceded by another process, such that there is no interval of time between the two processes.</rdfs:comment>
        <owl:inverseOf rdf:resource="&ss;immediatelyPrecedes"/>
        <rdfs:subPropertyOf rdf:resource="&ss;precededBy"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/immediatelyPrecedes -->

    <owl:ObjectProperty rdf:about="&ss;immediatelyPrecedes">
        <rdfs:label rdf:datatype="&xsd;string"
            >immediately precedes</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A non-transitive temporal relation in which one process is immediately precedes another process, such that there is no interval of time between the two processes.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&ss;precedes"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/inheresIn -->

    <owl:ObjectProperty rdf:about="&ss;inheresIn">
        <rdfs:label>inheres in</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&ss;isBorneBy"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isAbout -->

    <owl:ObjectProperty rdf:about="&ss;isAbout">
        <rdfs:label>is about</rdfs:label>
        <ss:definition
            >i is about c where i is an information content entity or an information carrier and c is an independent continuant.</ss:definition>
        <rdfs:subPropertyOf rdf:resource="&ss;isBorneBy"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isAdjacentTo -->

    <owl:ObjectProperty rdf:about="&ss;isAdjacentTo">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">is adjacent to</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >Two continuants are adjacent to each other if the edges of their spatial regions are in direct contact.</ss:definition>
        <rdfs:comment
            >RCC8:external connection</rdfs:comment>
        <owl:inverseOf rdf:resource="&ss;isAdjacentTo"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isConnectedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isAfter -->

    <owl:ObjectProperty rdf:about="&ss;isAfter">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is after</rdfs:label>
        <ss:definition
            >A spatial relation such that when x is after y, then x is positioned numerically greater than y on some spatial coordinate scale.</ss:definition>
        <owl:inverseOf rdf:resource="&ss;isBefore"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isConnectedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isAgentIn -->

    <owl:ObjectProperty rdf:about="&ss;isAgentIn">
        <rdf:type rdf:resource="&owl;AsymmetricProperty"/>
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:label>is agent in</rdfs:label>
        <ss:definition
            >A relation in which a continuant actively participates in the process.</ss:definition>
        <owl:inverseOf rdf:resource="&ss;hasAgent"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isParticipantIn"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isBearerOf -->

    <owl:ObjectProperty rdf:about="&ss;isBearerOf">
        <rdfs:label rdf:datatype="&xsd;string">bearer of</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A relation between an independent continuant and dependent continuant.</ss:definition>
        <ss:example
            >hair bearerOf brown color</ss:example>
        <owl:inverseOf rdf:resource="&ss;isBorneBy"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isRelatedTo"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&ss;isBearerOf"/>
            <rdf:Description rdf:about="&ss;isParticipantIn"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isBefore -->

    <owl:ObjectProperty rdf:about="&ss;isBefore">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is before</rdfs:label>
        <ss:definition
            >A spatial relation such that when x is before y, then x is positioned numerically less than y on some spatial coordinate scale.</ss:definition>
        <rdfs:subPropertyOf rdf:resource="&ss;isConnectedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isBorneBy -->

    <owl:ObjectProperty rdf:about="&ss;isBorneBy">
        <rdfs:label rdf:datatype="&xsd;string">borne by</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A relation between a dependent continuant and an entity.</ss:definition>
        <ss:example
            >brown colour inheresIn hair</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;isRelatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isConcretizationOf -->

    <owl:ObjectProperty rdf:about="&ss;isConcretizationOf">
        <rdfs:label
            >is concretization of</rdfs:label>
        <ss:example
            >The ink on the signpost forming the word &#39;Boston&#39; is a concretization of the word Boston that (generically) denotes the city of Boston.</ss:example>
        <ss:definition
            >s is a concretization of g, where s is a specifically dependent continuant and g is a generically dependent continuant.</ss:definition>
        <owl:inverseOf rdf:resource="&ss;isConcretizedBy"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isRelatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isConcretizedBy -->

    <owl:ObjectProperty rdf:about="&ss;isConcretizedBy">
        <rdfs:label
            >is concretized by</rdfs:label>
        <ss:example
            >The word &#39;Boston&#39; is concretized by the specific pattern of ink on the various signs around the city of Boston.</ss:example>
        <ss:definition
            >g is concretized by s, where g is a generically dependent continuant and s is a specifically dependent continuant.</ss:definition>
        <rdfs:subPropertyOf rdf:resource="&ss;isRelatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isConnectedTo -->

    <owl:ObjectProperty rdf:about="&ss;isConnectedTo">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is connected to</rdfs:label>
        <ss:definition
            >Two objects are connected to each other if their spatial regions are indirectly connected via a sequence of direct connections.</ss:definition>
        <owl:inverseOf rdf:resource="&ss;isConnectedTo"/>
        <rdfs:subPropertyOf rdf:resource="&ss;isSpatiallyRelatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isContainedIn -->

    <owl:ObjectProperty rdf:about="&ss;isContainedIn">
        <rdfs:label rdf:datatype="&xsd;string">is contained in</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A spatial relation between two continuants such that one is contained in the other, but for which there is no overlap and one is not a part of the other.</ss:definition>
        <rdfs:subPropertyOf rdf:resource="&ss;isSpatiallyRelatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isDenotedBy -->

    <owl:ObjectProperty rdf:about="&ss;isDenotedBy">
        <rdfs:label>is denoted by</rdfs:label>
        <ss:definition
            >c is denoted by g when a generically dependent continuant g is concretized by a specifically dependent continuant s which inheres in independent continuant c.</ss:definition>
        <rdfs:subPropertyOf rdf:resource="&ss;isSubjectOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isDisconnectedFrom -->

    <owl:ObjectProperty rdf:about="&ss;isDisconnectedFrom">
        <rdfs:label
            >is disconnected from</rdfs:label>
        <ss:definition
            >Two continuants are disconnected when their spatial regions are not adjacent, do not intersect or overlap.</ss:definition>
        <rdfs:comment
            >RCC8:disconnection</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&ss;isSpatiallyRelatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isDispositionOf -->

    <owl:ObjectProperty rdf:about="&ss;isDispositionOf">
        <rdfs:label rdf:datatype="&xsd;string"
            >is disposition of</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A relation between a disposition and the entity that realizes it.</ss:definition>
        <ss:example
            >disease isDispositionOf human</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;inheresIn"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isFunctionOf -->

    <owl:ObjectProperty rdf:about="&ss;isFunctionOf">
        <rdfs:label rdf:datatype="&xsd;string">is function of</rdfs:label>
        <ss:definition rdf:datatype="&xsd;string"
            >A relation between a function and the entity that realizes it.</ss:definition>
        <ss:example
            >Catalysis isFunctionOf Enzyme</ss:example>
        <rdfs:subPropertyOf rdf:resource="&ss;isDispositionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/isImmediatelyAfter -->

