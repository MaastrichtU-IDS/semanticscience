        <rdfs:label>object boundary</rdfs:label>
        <rdfs:subClassOf rdf:resource="&semanticscience;IndependentContinuant"/>
        <semanticscience:definition
            >The region of an object that exists exactly at the point where the object is separated from other entities.</semanticscience:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/Occurrent -->

