        <owl:disjointWith rdf:resource="&semanticscience;ScatteredSpatialRegion"/>
        <semanticscience:example
            >The space occupied by a cell, the space occupied by a molecule.</semanticscience:example>
        <semanticscience:definition
            >A contiguous region of space.</semanticscience:definition>
    </owl:Class>
    


    <!-- http://semanticscience.org/ConnectedTemporalRegion -->

    <owl:Class rdf:about="&semanticscience;ConnectedTemporalRegion">
        <rdfs:label
