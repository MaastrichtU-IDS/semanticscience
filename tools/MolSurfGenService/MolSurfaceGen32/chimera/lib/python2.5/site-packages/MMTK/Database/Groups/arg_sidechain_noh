pdbmap = [('ARG', {'CD': C_delta, 'NE': N_epsilon, 'CG': C_gamma, 'CB': C_beta, 'NH2': N_eta_2, 'NH1': N_eta_1, 'CZ': C_zeta, }, ), ]
