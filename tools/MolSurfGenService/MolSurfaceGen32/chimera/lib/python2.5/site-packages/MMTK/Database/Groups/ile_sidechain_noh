C_beta = Atom('CH')
C_delta_1 = Atom('CH3')
C_gamma_1 = Atom('CH2')
C_gamma_2 = Atom('CH3')
bonds = [Bond(C_gamma_2, C_beta), Bond(C_gamma_1, C_beta), Bond(C_delta_1, C_gamma_1), ]
