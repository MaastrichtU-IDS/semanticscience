C = Atom('C')
C_alpha = Atom('CH')
N = Atom('N')
O = Atom('O')
bonds = [Bond(N, C_alpha), Bond(C_alpha, C), Bond(C, O), ]
pdbmap = [('', {'N': N, 'O': O, 'CA': C_alpha, 'C': C, }, ), ]
amber_atom_type = {C_alpha: 'CT', O: 'O', C: 'C', N: 'N', }
pdb_alternative = {'HN': 'H', }
name = 'peptide in proline'
