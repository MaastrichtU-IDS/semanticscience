        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <terms:description xml:lang="en">A is contained in B iff the spatial region occupied by A is part of the spatial region occupied by B and A is not part of B. [T]</terms:description>
        <resource:equivalentTo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OBO_REL:contained_in</resource:equivalentTo>
        <resource:subset rdf:datatype="http://www.w3.org/2001/XMLSchema#string">core</resource:subset>
        <rdfs:isDefinedBy rdf:resource="http://semanticscience.org/ontology/sio/v1.43/sio-subset-core.owl"/>
        <rdfs:label xml:lang="en">is contained in</rdfs:label>
    </owl:ObjectProperty>
    


